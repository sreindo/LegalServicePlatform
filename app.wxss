/**app.wxss**/
page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
page{
  /* 定义主题颜色 */
  --themeColor:#4db34d;
  /* 定义统一的字体大小 */
  font-size: 28rpx;
  height: 100%;
}
.container {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

